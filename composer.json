{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for ZF2",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "require": {
        "php": "^7.0.2",
        "zendframework/zendframework": "^3.0",
        "phpunit/dbunit": "^3.0",
        "doctrine/doctrine-orm-module": "^1.1",
        "symfony/serializer": "^3.3",
        "symfony/property-access": "^3.3",
        "ext-PDO": "^7.2",
        "ext-json": "^1.6",
        "ext-SimpleXML": "^7.2"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Logging\\": "module/Logging/src/",
            "ErrorHandling\\": "module/ErrorHandling/src/",
            "Authentication\\": "module/Authentication/src/",
            "Authorization\\": "module/Authorization/src/",
            "AuditLogging\\": "module/AuditLogging/src/",
            "MasterData\\": "module/MasterData/src/",
            "Order\\": "module/Order/src/",
            "Application\\": "module/Application/src/",
            "Base\\": "module/Base/src/"
        }
    }
}
